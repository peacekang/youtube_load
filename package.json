{
  "name": "project",
  "version": "1.0.0",
  "private": true,
  "description": "项目",
  "scripts": {
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "build": "cross-env NODE_ENV=production vue-cli-service build",
    "commit": "git cz",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint **/*.{html,vue,css,sass,scss} --fix",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "pack": "vue-cli-service build && node ./deploy/tar.js",
    "serve": "vue-cli-service serve",
    "tar": "node ./deploy/tar.js",
    "tar:skip": "node ./deploy/tar.js skip"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{html,vue,css,sass,scss}": [
      "stylelint --cache **/*.{html,vue,css,sass,scss} --fix",
      "git add ."
    ],
    "*.{ts,vue}": [
      "prettier --write",
      "vue-cli-service lint",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.13.2",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "autoprefixer": "^9.8.6",
    "babel-plugin-component": "^1.1.1",
    "commitizen": "^4.2.1",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.4.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.3.3",
    "stylelint": "^13.7.2",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "tar-fs": "^2.1.0",
    "typescript": "~3.9.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-spritesmith": "^1.1.0",
    "zlib": "^1.0.5"
  }
}
